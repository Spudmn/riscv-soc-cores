CAPI=1

[main]
name = de10-nano-bus-spider
description = DE10-Nano Bus Spider Design for InnovateFPGA-2018

depend =
  altera_clkgen
  bus-spider-soc

backend = quartus

[quartus]
family = "Cyclone V"
device = 5CSEBA6U23I7
sdc_files = data/de10-nano-bus-spider.sdc
tcl_files = data/de10-nano-bus-spider.qsf
qsys_files = data/soc_system.qsys
top_module = de10_nano_bus_spider
quartus_options = --64bit

[verilog]
src_files =
  rtl/verilog/de10-nano-bus-spider.v
