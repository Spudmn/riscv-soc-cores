CAPI=1

[main]
name = marsohod2-picorv32-wb-soc
description = RISC-V SoC for Marsohod2

depend =
  picorv32-wb-soc
  riscv-nmon

backend = quartus

[quartus]
family = "Cyclone III"
device = EP3C10E144C8
sdc_files = data/marsohod2.sdc
tcl_files = data/pinmap.tcl
            data/options.tcl
top_module = marsohod2_picorv32_wb_soc

[verilog]
src_files =
  rtl/verilog/aux.v
  rtl/verilog/marsohod2_picorv32_wb_soc.v
  rtl/verilog/cyclone3-altpll/altpll0.v
