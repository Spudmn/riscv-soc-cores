CAPI=1

[main]
name = marsohod2rpi-picorv32-wb-soc
description = RISC-V SoC for Marsohod2RPI

depend =
  altera_clkgen
  picorv32-wb-soc
  riscv-nmon

backend = quartus

[quartus]
family = "Cyclone IV E"
device = EP4CE6E22C8
sdc_files = data/marsohod2.sdc
tcl_files = data/marsohod2rpi-pinmap.tcl
            data/options.tcl
top_module = marsohod2rpi_picorv32_wb_soc

[verilog]
src_files =
  rtl/verilog/marsohod2rpi_picorv32_wb_soc.v
